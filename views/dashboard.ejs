<!-- GRAICO DOS CLIENTES -->
<script>
  async function get_clientes(){
    const clientes_label = []
    const response = await fetch('/auth/clientsGET/')
    const data = await response.json()
    const valores_true = []
    const valores_false = []
    for (i=0; i<data.length;i++){
      if(data[i].hasAdmin === true){
        valores_true.push(data[i])
      }
    }
    for (i=0; i<data.length;i++){
      if(data[i].hasAdmin === false){
        valores_false.push(data[i])
      }
    }
    clientes_label.push(valores_true.length,valores_false.length)
    document.getElementById("clientes_total").innerHTML = "Total de clientes: " + data.length
    return {clientes_label};
  }
  async function get_clientes_chart() {
    const data = await get_clientes();
    const ctx = document.getElementById('chart_clientes_canvas').getContext('2d');
    const myChart = new Chart(ctx, {
      type: 'bar',
      data: {
        labels: ["Clientes com manager", "Clientes sem manager"],
        datasets: [{
          data: data.clientes_label,
          backgroundColor: [
            'rgba(255, 99, 132, 0.2)',
            'rgba(54, 162, 235, 0.2)',
            //'rgba(255, 206, 86, 0.2)',
            //'rgba(75, 192, 192, 0.2)',
            //'rgba(153, 102, 255, 0.2)',
            //'rgba(255, 159, 64, 0.2)'
          ],
          borderColor: [
            'rgba(255, 99, 132, 1)',
            'rgba(54, 162, 235, 1)',
            //'rgba(255, 206, 86, 1)',
            //'rgba(75, 192, 192, 1)',
            //'rgba(153, 102, 255, 1)',
            //'rgba(255, 159, 64, 1)'
          ],
          borderWidth: 1,
        }]
      },
      options: {
        responsive: true,
        legend:false,
        scales: {
          yAxes: [{
            ticks:{
              autoSkip: false,
              beginAtZero: true,
            }
          }]
        }
      }
    });
  }
  get_clientes_chart();
</script>
