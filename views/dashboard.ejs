<script>
  async function get_trabalhadores(){
    const trabalhadores = await fetch('/auth/workersGET/')
    const data_trabalhadores = await trabalhadores.json()
    const valores_solo = []
    for (i=0; i<data_trabalhadores.length;i++){
      if(data_trabalhadores[i].pequipa === "Não"){
        valores_solo.push(data_trabalhadores[i])
      }
    }
    document.getElementById("trabalhadores_total").innerHTML = "Total de trabalhadores: " + data_trabalhadores.length
    const trabalhadores_size = valores_solo.length
    return {trabalhadores_size};
  }
  async function get_equipas(){
    const equipas = await fetch('/auth/equipasGET/')
    const data_equipas = await equipas.json()
    const equipas_size = data_equipas.length
    return {equipas_size};
  }
  async function get_trabalhadores_chart() {
    const data_equipas = await get_equipas();
    const data_trabalhador = await get_trabalhadores();
    const ctx = document.getElementById('chart_trabalhadores_canvas').getContext('2d');
    const myChart = new Chart(ctx, {
      type: 'bar',
      data: {
        labels: ["Trabalhadores soltos", "Equipas"],
        datasets: [{
          data: [data_trabalhador.trabalhadores_size,data_equipas.equipas_size],
          backgroundColor: [
            'rgba(255, 99, 132, 0.2)',
            'rgba(54, 162, 235, 0.2)',
            //'rgba(255, 206, 86, 0.2)',
            //'rgba(75, 192, 192, 0.2)',
            //'rgba(153, 102, 255, 0.2)',
            //'rgba(255, 159, 64, 0.2)'
          ],
          borderColor: [
            'rgba(255, 99, 132, 1)',
            'rgba(54, 162, 235, 1)',
            //'rgba(255, 206, 86, 1)',
            //'rgba(75, 192, 192, 1)',
            //'rgba(153, 102, 255, 1)',
            //'rgba(255, 159, 64, 1)'
          ],
          borderWidth: 1,
        }]
      },
      options: {
        responsive: true,
        legend:false,
        scales: {
          yAxes: [{
            ticks:{
              autoSkip: false,
              beginAtZero: true,
            }
          }]
        }
      }
    });
  }
  get_trabalhadores_chart();
</script>
