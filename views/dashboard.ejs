<script>
  async function get_avaliacoes(){
    const notas_de_avaliacoes = []
    const avaliacoes = await fetch('/auth/marcacoesGETAll/')
    const data_avaliacoes = await avaliacoes.json()
    const uma_estrela = []
    const duas_estrela = []
    const tres_estrela = []
    const quatro_estrela = []
    const cinco_estrela = []
    const total_sem_avaliacao = []
    for (let i=0; i<data_avaliacoes.length;i++){
      if (data_avaliacoes[i].aval_client ==="Sem Avaliação do Cliente"){
        total_sem_avaliacao.push(data_avaliacoes[i])
      }
    }
    for (let i=0; i<data_avaliacoes.length;i++){
      if(data_avaliacoes[i].aval_client === "1"){
        uma_estrela.push(data_avaliacoes[i])
      }else if(data_avaliacoes[i].aval_client === "2"){
        duas_estrela.push(data_avaliacoes[i])
      }else if(data_avaliacoes[i].aval_client === "3"){
        tres_estrela.push(data_avaliacoes[i])
      }else if(data_avaliacoes[i].aval_client === "4"){
        quatro_estrela.push(data_avaliacoes[i])
      }else if(data_avaliacoes[i].aval_client === "5"){
        cinco_estrela.push(data_avaliacoes[i])
      }
    }
    const total_com_aval = data_avaliacoes.length - total_sem_avaliacao.length
    notas_de_avaliacoes.push(uma_estrela.length, duas_estrela.length, tres_estrela.length,quatro_estrela.length,cinco_estrela.length)
    //console.log(notas_de_avaliacoes)
    document.getElementById("avaliacoes_total").innerHTML = "Avaliados: " + total_com_aval
    return {notas_de_avaliacoes};
  }
