<script>
  async function get_admins_total(){
    const administrador = await fetch('/auth/allAdminsGET/')
    const data_admin = await administrador.json()
    const numero_de_admins = data_admin.length
    return {numero_de_admins}
  }
  async function get_clientes_total(){
    const response = await fetch('/auth/clientsGET/')
    const data_clientes = await response.json()
    const numero_de_clientes = data_clientes.length
    return {numero_de_clientes};
  }
  async function get_trabalhadores_total(){
    const trabalhadores = await fetch('/auth/workersGET/')
    const data_trabalhadores = await trabalhadores.json()
    const total_de_trabalhadores = data_trabalhadores.length
    return {total_de_trabalhadores}
  }
  async function get_marcacoes_total(){
    const avaliacoes = await fetch('/auth/marcacoesGETAll/')
    const data_avaliacoes = await avaliacoes.json()
    const total_sem_avaliacao = []
    for (let i=0; i<data_avaliacoes.length;i++){
      if (data_avaliacoes[i].aval_client ==="Sem Avaliação do Cliente"){
        total_sem_avaliacao.push(data_avaliacoes[i])
      }
    }
    const total_de_marcacoes = total_sem_avaliacao.length
    return {total_de_marcacoes};
  }
  async function get_novo_chart() {
    const data_admins = await get_admins_total();
    const data_clientes = await get_clientes_total();
    const data_trabalhadores = await get_trabalhadores_total();
    const data_marcacoes = await get_marcacoes_total();
    const admin_por_cliente =data_admins.numero_de_admins/data_clientes.numero_de_clientes
    const trabalhadores_por_marcacoes = data_trabalhadores.total_de_trabalhadores/data_marcacoes.total_de_marcacoes
    document.getElementById("comparacoes").innerHTML = "A/C: " + admin_por_cliente.toFixed(2) + " | " + "T/M: " + trabalhadores_por_marcacoes.toFixed(2)

    const ctx = document.getElementById('novo_chart').getContext('2d');
    const myChart = new Chart(ctx, {
      type: 'pie',
      data: {
        labels: ["Administradores", "Clientes", "Trabalhador", "Marcações"],
        datasets: [{
          data: [data_admins.numero_de_admins, data_clientes.numero_de_clientes, data_trabalhadores.total_de_trabalhadores, data_marcacoes.total_de_marcacoes],
          backgroundColor: [
            'rgba(255, 99, 132, 0.2)',
            'rgba(54, 162, 235, 0.2)',
            'rgba(255, 206, 86, 0.2)',
            'rgba(75, 192, 192, 0.2)',
            'rgba(153, 102, 255, 0.2)',
            'rgba(255, 159, 64, 0.2)'
          ],
          borderColor: [
            'rgba(255, 99, 132, 1)',
            'rgba(54, 162, 235, 1)',
            'rgba(255, 206, 86, 1)',
            'rgba(75, 192, 192, 1)',
            'rgba(153, 102, 255, 1)',
            'rgba(255, 159, 64, 1)'
          ],
          borderWidth: 1,
        }]
      },
    });
  }
  get_novo_chart();
</script>
